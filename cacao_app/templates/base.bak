{% load i18n  cacao_tags %}
<!DOCTYPE html>
<html>
    <!-- START Head -->
    <head>
        {% block head %}
        <!-- START META SECTION -->
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Caja de herramientas para cacao</title>
        <meta name="author" content="kronoscode.com/">
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{% static 'image/touch/apple-touch-icon-144x144-precomposed.png' %}">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{% static 'image/touch/apple-touch-icon-114x114-precomposed.png' %}">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{% static 'image/touch/apple-touch-icon-72x72-precomposed.png' %}">
        <link rel="apple-touch-icon-precomposed" href="{% static 'image/touch/apple-touch-icon-57x57-precomposed.png' %}">
        <link rel="shortcut icon" href="{% static 'image/favicon.ico' %}">
        <!--/ END META SECTION -->

        <!-- Application stylesheet : mandatory -->
        <link rel="stylesheet" href="{% static 'css/bootstrap.min.css' %}">
        <link rel="stylesheet" href="{% static 'css/layout.min.css' %}">
        <link rel="stylesheet" href="{% static 'css/uielement.min.css' %}">
        <link rel="stylesheet" href="{% static 'css/custom.css' %}">
        <link rel="stylesheet" href="{% static 'css/colors-guias.min.css' %}">
        <!--/ Application stylesheet -->
        <!-- END STYLESHEETS -->

        <!-- START JAVASCRIPT SECTION - Load only modernizr script here -->
        <script src="{% static 'js/modernizr.2.8.3.min.js' %}"></script>
        <!--/ END JAVASCRIPT SECTION -->
        {% endblock head %}

        {% block extra_head %}{% endblock extra_head %}

    </head>
    <!--/ END Head -->

    <!-- START Body -->
    <body>
        <!-- START Template Header -->
        <header id="header" class="navbar hidden-lg hidden-md">
            <!-- START navbar header -->
            <div class="navbar-header">
                <!-- Brand -->
                <a class="navbar-brand" href="javascript:void(0);">
                    <span class="logo-figure"></span>
                    <!-- <span class="logo-text"></span> -->
                    <span class="text-white hidden-sm " >Lutheran World Relief</span>
                </a>
                <!--/ Brand -->
            </div>
            <!--/ END navbar header -->

            <!-- START Toolbar -->
            <div class="navbar-toolbar clearfix">
                <!-- Brand sm -->
                <div class="navbar-brand navbar-brand-sm text-white visible-sm">
                    <span>Lutheran World Relief</span>
                </div>
                <!--/ Brand sm -->
                <!-- START Left nav -->
                <ul class="nav navbar-nav navbar-left">
                    <!-- Offcanvas left: This menu will take position at the top of template header (mobile only). Make sure that only #header have the `position: relative`, or it may cause unwanted behavior -->
                    <li class="navbar-main hidden-lg hidden-md hidden-sm">
                        <a href="javascript:void(0);" data-toggle="sidebar" data-direction="ltr" rel="tooltip" title="Menu sidebar">
                            <span class="meta">
                                <span class="icon"><i class="ico-paragraph-justify3"></i></span>
                            </span>
                        </a>
                    </li>
                    <!--/ Offcanvas left -->
                </ul>
                <!--/ END Left nav -->
            </div>
            <!--/ END Toolbar -->
        </header>
        <!--/ END Template Header -->

        <!-- START Template Sidebar (Left) -->
        <aside class="sidebar sidebar-left sidebar-menu" style="top: 0;">
            <div class="header hidden-xs text-center">
                <img src="{% static 'image/logo/logo.png' %}" width="180">
                <h5 class="heading hidden-sm ">
                    <span class="figure">
                        <i class="ico-paragraph-justify3"></i>
                    </span>
                    <span class="text">Menu</span>
                </h5>
            </div>

            <!-- START Sidebar Content -->

            <section class="content slimscroll menu-index">

                <!-- START Template Navigation/Menu -->
                <ul class="topmenu topmenu-responsive text-uppercase" data-toggle="menu">
                    <li class="home-menu">
                        <a href="{% url 'home' %}">
                            <span class="figure"><i class="ico-drawer"></i></span>
                            <span class="text">Lista de guías</span>
                        </a>
                    </li>
                    <li class="howworks-menu">
                        <a href="{% url 'how_it_works' %}">
                            <span class="figure"><i class="ico-cog3"></i></span>
                            <span class="text">Como funciona</span>
                        </a>
                    </li>
                    <li class="contact-menu">
                        <a href="{% url 'envelope-contact' %}">
                            <span class="figure"><i class="ico-envelope"></i></span>
                            <span class="text">Contáctanos</span>
                        </a>
                    </li>
                    <li class="about-menu">
                        <a href="{% url 'about' %}">
                            <span class="figure"><i class="ico-question3"></i></span>
                            <span class="text">Acerca de la aplicación</span>
                        </a>
                    </li>

                    <li id="js-offline-menu__home" class="hidden">
                        <a href="index.html">
                            <span class="figure"><i class="ico-drawer"></i></span>
                            <span class="text">Contenido</span>
                        </a>
                    </li>
                    <li id="js-offline-menu__about" class="hidden">
                        <a href="about.html">
                            <span class="figure"><i class="ico-question3"></i></span>
                            <span class="text">Acerca de la aplicación</span>
                        </a>
                    </li>
                </ul>
                <!--/ END Template Navigation/Menu -->

                <!-- START Sidebar summary -->
                <!-- Summary -->
                <h5 class="heading text-center js-offline__sidebar" style="margin-top:40px;">Caja de herramientas para Cacao también disponible para dispositivos android</h5>
                <div class="wrapper text-center js-offline__sidebar">
                    <div class="table-layout">
                        <img src="{% static 'image/ipad.png' %}" alt="ipad image">
                    </div>
                    <!-- START button google play -->
                    <div class="table-layout">
                        <a class="btn btn-google-play" href="https://play.google.com/store/apps/details?id=kronos.cacaomovil" target="_blank" >
                            <img src="{% static 'image/google-play.png' %}" alt="image google play image">
                        </a>
                    </div>
                    <!--/ END button google play -->
                </div>
                <!--/ Summary -->
                <!--/ END Sidebar summary -->
                <!-- Credit -->
                <hr>
                <div class="wrapper text-center text-white">
                    <div class="table-layout">
                        <span style="color: #696c6e;"><small>Desarrollado por:</small></span>
                        <a href="http://kronoscode.com/" >
                            <img src="{% static 'image/kronoscode.png' %}">
                        </a>
                    </div>
                </div>
                <!--/ Credit -->
            </section>
            <!--/ END Sidebar Container -->
        </aside>
        <!--/ END Template Sidebar (Left) -->
            {% block content %}
            {% endblock content %}
            <!-- START To Top Scroller -->
            <a href="#" class="totop animation" data-toggle="waypoints totop" data-showanim="bounceIn" data-hideanim="bounceOut" data-offset="50%"><i class="ico-angle-up"></i></a>
            <!--/ END To Top Scroller -->
        </section>
        <!--/ END Template Main -->
        {% block footer %}
        <!-- START JAVASCRIPT SECTION (Load javascripts at bottom to reduce load time) -->
        <!-- Library script : mandatory -->
        <script type="text/javascript" src="{% static 'js/jquery.min.js' %}"></script>
        <script type="text/javascript" src="{% static 'js/jquery-migrate.min.js' %}"></script>
        <script type="text/javascript" src="{% static 'js/bootstrap.min.js' %}"></script>
        <script type="text/javascript" src="{% static 'js/core.js' %}"></script>
        <!--/ Library script -->

        <!-- App and page level script -->
         <!-- <script type="text/javascript" src="./js/jquery.sparkline.min.js"></script> -->
        <!-- will be use globaly as a summary on sidebar menu -->
        <script type="text/javascript" src="{% static 'js/app.js' %}"></script>

        <!--/ App and page level script -->
        <!--/ END JAVASCRIPT SECTION -->
        {% endblock footer %}
        {% block extra_footer %}{% endblock %}

        <!-- Analytics -->
        <script type="text/javascript">
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', '{% settings_value "GA_APP_ID" %}', 'auto');
          ga('send', 'pageview');

        </script>
        <script type="text/javascript">
            if (window.location.protocol != "http:") {
                $('#js-offline-menu__about').removeClass('hidden');
                $('#js-offline-menu__home').removeClass('hidden');
                $('.about-menu').addClass('hidden');
                $('.home-menu').addClass('hidden');
                $('.howworks-menu').addClass('hidden');
                $('.contact-menu').addClass('hidden');
                $('.js-offline__sidebar').addClass('hidden');
            }
        </script>
    </body>
    <!--/ END Body -->
</html>
